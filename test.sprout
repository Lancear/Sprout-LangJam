// FizzBuzz
fn main(): int {
  for(mut i: int = 1; i <= 25; i++) {
    if (i % 3 == 0) Console.write("Fizz");
    if (i % 5 == 0) Console.write("Buzz");
    if (i % 3 != 0 && i % 5 != 0) Console.write("" + i);
    Console.writeln("");
  }
  
  return 7;
}


